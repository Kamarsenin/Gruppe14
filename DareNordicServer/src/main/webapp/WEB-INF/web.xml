<?xml version="1.0" encoding="UTF-8"?>
<web-app
	xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_4.xsd"
    version="2.4">
	<display-name>My Webapp</display-name>
  
  
  	//User_Servlet
	<servlet>
		<servlet-name>SearchUsersServlet</servlet-name>
		<servlet-class>no.nith.SearchUsersServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SearchUsersServlet</servlet-name>
		<url-pattern>/user</url-pattern>
	</servlet-mapping>
  
  
	//UserLisence_Servlet
	<servlet>
		<servlet-name>SearchUserLisenceServlet</servlet-name>
		<servlet-class>no.nith.SearchUserLisenceServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SearchUserLisenceServlet</servlet-name>
		<url-pattern>/userLisence</url-pattern>
	</servlet-mapping>
	
  
  
</web-app>
